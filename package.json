    "@react-navigation/stack": "^6.3.17",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "firebase": "^9.23.0",
    "formik": "^2.4.2",
    "moment": "^2.29.4",
    "react": "18.2.0",
    "react-native": "0.72.0",
